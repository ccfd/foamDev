FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object	omega;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 -1 0 0 0 0];

internalField   uniform 100;

boundaryField
{
    wall_top
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    wall_bottom
    {
        type            omegaWallFunction;
        value           $internalField;
    }

    pressure_outlet
    {
        type            zeroGradient;
    }

    velocity_inlet
    {
        type            fixedValue;
	value		uniform 438.5805;
    }

    top_1
    {
        type            cyclicAMI;
    }

    bottom_1
    {
        type            cyclicAMI;
    }

    frontAndBackPlanes
    {
        type            empty;
    }

}

// ************************************************************************* //
