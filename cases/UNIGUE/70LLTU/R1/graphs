set term png



path="postProcessing/sample/".time."/"
out="../experiment/"



# ------ 0.3
#U
set output "images/U_x03.png"
set title "U, x = 0.3"
set xlabel "y [m]"
plot path."x=03_U.xy" using 2:4 title 'U' with lines, out."x=0.3.xy" using 2:3 title 'Uexp'

#V
set output "images/V_x03.png"
set title "V, x = 0.3"
set xlabel "y [m]"
plot path."x=03_U.xy" using 2:5 title 'V' with lines, out."x=0.3.xy" using 2:4 title 'Vexp'

#k
set output "images/k_x03.png"
set title "k, x = 0.3"
set xlabel "y [m]"
plot path."x=03_p_k.xy" using 2:5 title 'k' with lines, out."x=0.3.xy" using 2:5 title 'kexp'

