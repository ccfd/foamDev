FoamFile
{
    version     2.0;
    format      ascii;
    class       volScalarField;
    location    "0";
    object      nut;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 2 -1 0 0 0 0];

internalField   uniform 1;

boundaryField
{
    BA
    {
        type            calculated;
        value           $internalField;
    }

    CB
    {
        type            calculated;
        value           $internalField;
    }

    DC
    {
        type            calculated;
        value           $internalField;
    }

    AD
    {
        type            calculated;
        value           $internalField;
    }


    // Upper
    // {
    //     type            calculated;
    //     value           $internalField;
    // }

    // Lower
    // {
    //     type            calculated;
    //     value           $internalField;
    // }



    frontAndBack
    {
        type            empty;
    }
}


