FoamFile
{
    version     2.0;
    format      ascii;
    class       volVectorField;
    location    "0";
    object      U;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 1 -1 0 0 0 0];

internalField   uniform (0 0 0);

boundaryField
{
    wall_top
    {
        type            noSlip;
    }

    wall_bottom
    {
        type            noSlip;

    }

    pressure_outlet
    {
        type            zeroGradient;
    }

    velocity_inlet
    {
        type            fixedValue;
	value		uniform (8.2 0 0); 
    }

    top_1
    {
        type            cyclicAMI;
    }

    bottom_1
    {
        type            cyclicAMI;
    }

    frontAndBackPlanes
    {
        type            empty;
    }

}

